{
  "name": "missing-people",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "fullhuman/postcss-purgecss",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-transition-group": "^4.3.0",
    "tailwindcss-grid": "^1.2.1",
    "typeface-montserrat": "^0.0.75"
  },
  "scripts": {
    "build:css": "postcss src/index.css -o src/styles.css",
    "watch:css": "postcss src/index.css -o src/styles.css -w",
    "lint": "yarn lint:scripts",
    "lint:scripts": "eslint .",
    "start:scripts": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "concurrently --kill-others \"yarn start:scripts\" \"yarn watch:css\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "tailwindcss": "^1.1.4"
  }
}
